frontend http_front
    bind *:80
    bind *:443 crt /etc/haproxy/certs/www.realemildawood.tech.pem
    mode http
    http-request redirect scheme https code 301 if !{ ssl_fc }
    default_backend webservers
backend webservers
    mode http
    balance roundrobin
    server 387530-web-01 54.89.57.165:80 check
    server 387530-web-02 18.207.142.135:80 check
